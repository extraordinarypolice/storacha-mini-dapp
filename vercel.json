{
  "version": 2,
  "builds": [
    {
      "src": "storacha-backend/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "storacha-backend/api/$1.js" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
